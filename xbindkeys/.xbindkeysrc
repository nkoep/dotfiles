"mpc toggle"
  Mod2 + F7

"mpc prev"
  Mod2 + F9

"mpc next"
  Mod2 + F10

"pactl set-sink-volume @DEFAULT_SINK@ +5%"
  Mod2 + F11

"pactl set-sink-volume @DEFAULT_SINK@ -5%"
  Mod2 + F12

"xbacklight + 5%"
  Alt + Up

"xbacklight - 5%"
  Alt + Down

"slock"
  Alt + F5

"xkill"
  Alt + Shift + x

"maim -s | xclip -selection clipboard -t image/png"
  Shift + Control + 3

"maim | xclip -selection clipboard -t image/png"
  Shift + Control + 4
